{
    "name": "payzeio/laravel-payze",
    "description": "Payze.io Payment Integration for Laravel",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6|^7",
        "illuminate/support": ">=5.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Levan Lotuashvili",
            "email": "l.lotuashvili@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PayzeIO\\LaravelPayze\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PayzeIO\\LaravelPayze\\PayzeServiceProvider"
            ],
            "aliases": {
                "Payze": "PayzeIO\\LaravelPayze\\Facades\\Payze"
            }
        }
    }
}
